{
  "id": "helloworld",
  "version": "1.0",
  "name": "Hello World Workflow",
  "scope": "private",
  "description": "Inject Hello World",
  "states":[
    {
      "name":"Hello State",
      "type":"inject",
      "start": {
        "kind": "default"
      },
      "data": {
        "result": "Hello World!"
      },
      "end": {
        "kind": "default"
      }
    }
  ]
}